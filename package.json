{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && next dev",
    "build": "next build",
    "build:prod": "NODE_ENV=production yarn build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "testu": "jest -u",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "test:watch": "yarn test --watch",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --browser chrome --headless",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "test:e2e:ci": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "fakeserver": "json-server server.json -w -d 750 -p 3333"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "date-fns": "^2.21.1",
    "next": "10.1.3",
    "next-pwa": "^5.2.9",
    "rc-slider": "^9.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "cypress": "^7.1.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "json-server": "^0.16.3",
    "lint-staged": "^10.5.4",
    "msw": "^0.28.2",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.12.1",
    "typescript": "^4.2.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
